{
  "name": "reggae-finder/artists",
  "description": "A package modeling artists, musicians and bands",
  "license": "MIT",
  "require": {
    "beberlei/assert": "3.3.*",
    "reggae-finder/uuid": "@dev"
  },
  "require-dev": {
    "phpunit/phpunit": "9.5.*",
    "phpstan/phpstan": "0.12.87",
    "phpmetrics/phpmetrics": "2.7.*",
    "reggae-finder/uuid-testing": "@dev",
    "php-mock/php-mock-phpunit": "^2.6"
  },
  "autoload": {
    "psr-4": {
      "ReggaeFinder\\Domain\\Artists\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ReggaeFinder\\Domain\\Artists\\Tests\\": "tests"
    }
  },
  "repositories":[{
    "type": "path",
    "url": "../../../{*,*/*}/*"
  }],
  "minimum-stability": "dev",
  "prefer-stable": true
}

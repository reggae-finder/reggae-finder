{
  "name": "reggae-finder/artists-doctrine-bridge",
  "description": "Doctrine integration for the reggae-finder/artists package",
  "license": "MIT",
  "require": {
    "doctrine/orm": "2.8.*",
    "reggae-finder/artists": "@dev",
    "reggae-finder/doctrine-utils": "@dev",
    "reggae-finder/uuid-doctrine": "@dev"
  },
  "require-dev": {
    "doctrine/data-fixtures": "^1.5",
    "nyholm/nsa": "1.2.*",
    "phpstan/phpstan": "0.12.87",
    "phpmetrics/phpmetrics": "2.7.*",
    "phpunit/phpunit": "9.5.*",
    "reggae-finder/artists-testing-utils": "@dev",
    "reggae-finder/doctrine-testing-utils": "@dev"
  },
  "autoload": {
    "psr-4": {
      "ReggaeFinder\\Bridges\\Artists\\Doctrine\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ReggaeFinder\\Bridges\\Artists\\Doctrine\\Fixtures\\": "fixtures",
      "ReggaeFinder\\Bridges\\Artists\\Doctrine\\Tests\\": "tests"
    }
  },
  "repositories":[{
    "type": "path",
    "url": "../../../{*,*/*}/*"
  }],
  "minimum-stability": "dev",
  "prefer-stable" : true
}

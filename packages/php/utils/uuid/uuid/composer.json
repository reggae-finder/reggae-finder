{
  "name": "reggae-finder/uuid",
  "description": "A small wrapper around ramsey/uuid focused on V4",
  "type": "library",
  "license": "MIT",
  "require": {
    "ramsey/uuid": "4.1.*"
  },
  "require-dev": {
    "phpunit/phpunit": "9.5.*",
    "phpstan/phpstan": "0.12.87",
    "phpmetrics/phpmetrics": "2.7.*",
    "reggae-finder/uuid-testing": "@dev"
  },
  "autoload": {
    "psr-4": {
      "ReggaeFinder\\Utils\\Uuid\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ReggaeFinder\\Utils\\Uuid\\Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable" : true,
  "repositories":[{
    "type": "path",
    "url": "../../../{*,*/*}/*"
  }]
}

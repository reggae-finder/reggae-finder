{
  "name": "reggae-finder/uuid-doctrine",
  "description": "Integrate uuid in Doctrine",
  "license": "MIT",
  "require": {
    "reggae-finder/uuid": "@dev",
    "doctrine/orm": "2.8.*",
    "reggae-finder/uuid-testing": "@dev"
  },
  "require-dev": {
    "phpunit/phpunit": "9.5.*",
    "phpstan/phpstan": "0.12.87",
    "phpmetrics/phpmetrics": "2.7.*"
  },
  "autoload": {
    "psr-4": {
      "ReggaeFinder\\Utils\\UuidDoctrine\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ReggaeFinder\\Utils\\UuidDoctrine\\Tests\\": "tests"
    }
  },
  "repositories":[{
    "type": "path",
    "url": "../../../{*,*/*}/*"
  }],
  "minimum-stability": "dev",
  "prefer-stable" : true
}
